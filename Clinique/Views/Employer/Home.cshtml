@using AspNetCoreTemplate.C_
@model dynamic

@{
    Layout = RoutePath.ContentEmployer();
    Utilisateur utilisateur = Model.utilisateur;
}

@utilisateur

<div class="card top-selling overflow-auto">
    <div class="card-body pb-0">
        <h5 class="card-title">Import CSV</h5>
        
        <form asp-controller="Employer" asp-action="Csv" enctype="multipart/form-data">
            <input type="file" name="fichier" accept=".csv"/>
            <input type="submit"/>
        </form>
        
    </div>
</div>